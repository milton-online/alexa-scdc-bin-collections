{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "es6": true,
        "mocha": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "no-unused-vars": [
            "error",
            { "varsIgnorePattern": "should" }
        ]
    }
}
